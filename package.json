{
  "name": "webapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "local": "nuxt generate && nuxt",
    "deploy": "push-dir --dir=dist --branch=master --cleanup",
    "clean": "rm -rf node_modules && rm package-lock.json && yarn cache clean --force",
    "reinstall": "yarn run clean && yarn install --force",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn run lint:js"
  },
  "dependencies": {
    "@nuxt/content": "^1.13.0",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/sitemap": "^2.4.0",
    "@tailwindcss/typography": "^0.4.0",
    "core-js": "^3.6.5",
    "eslint-plugin-vue": "^7.5.0",
    "faunadb": "^4.1.0",
    "nuxt": "^2.14.8",
    "prism-themes": "^1.5.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "postcss": "^7.0.35",
    "push-dir": "^0.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  }
}
